            <mappings>
                <mapping>
                    <value>1</value>
                    <newvalue>outOfRange</newvalue>
                </mapping>
                <mapping>
                    <value>2</value>
                    <newvalue>inRange</newvalue>
                </mapping>
            </mappings>
        </value_map>
        <value_map>
            <name>atEnvMonv2VoltageStatus</name>
            <mappings>
                <mapping>
                    <value>1</value>
                    <newvalue>outOfRange</newvalue>
                </mapping>
                <mapping>
                    <value>2</value>
                    <newvalue>inRange</newvalue>
                </mapping>
            </mappings>
        </value_map>
    </value_maps>
